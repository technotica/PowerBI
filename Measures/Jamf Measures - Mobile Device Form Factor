###### Jamf Measures for PowerBI - Mobile Devices
# Use these Measures to obtain count information Mobile Devices 

###========================================###

# Mobile Devices
All iPads = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad",'Mobile Devices'[mobileDeviceDetails.model],1,0)))

iPad Standard = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 3rd",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 4th",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 5th",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 6th",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 7th",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 8th",'Mobile Devices'[mobileDeviceDetails.model],1,0)))  + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 9th",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad 10th",'Mobile Devices'[mobileDeviceDetails.model],1,0)))

iPad Air = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Air",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

iPad Mini = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Mini",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

iPad Pro = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

iPhone = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPhone",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

iPod Touch = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPod touch",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

Apple TV = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("Apple TV",'Mobile Devices'[mobileDeviceDetails.model],1,0)))

# iPad Pros
iPad Pro 9.7-inch = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro (9.7-inch",'Mobile Devices'[mobileDeviceDetails.model],1,0)))

iPad Pro 10.5-inch = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro 2nd Generation (10.5-inch",'Mobile Devices'[mobileDeviceDetails.model],1,0))) 

iPad Pro 11-inch = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro (11-",'Mobile Devices'[mobileDeviceDetails.model],1,0)))

iPad Pro 12.9-inch = CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro 2nd Generation (12.9",'Mobile Devices'[mobileDeviceDetails.model],1,0))) + CALCULATETABLE(ROW("rows",COUNT('Mobile Devices'[mobileDeviceDetails.model])),FILTER(ALL('Mobile Devices'[mobileDeviceDetails.model]),SEARCH("iPad Pro (12.9",'Mobile Devices'[mobileDeviceDetails.model],1,0)))